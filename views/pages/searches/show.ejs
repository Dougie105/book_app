<!-- display my search results here -->

<!DOCTYPE html>
<html>
<head>
  <title>Book List</title>
</head>
<body>
  <% searchResults.forEach( book => { %>
    <p>Title: <%= book.title %></p>
    <p>Author: <%= book.authors %></p>
    <p>Description: <%= book.description %></p>
    <% }); %>
</body>
</html>